{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"no-console": 1,
		"no-unused-vars": 1,
		"no-fallthrough": 1,
		"no-mixed-spaces-and-tabs": 1
	},
	"extends": "eslint:recommended"
}
